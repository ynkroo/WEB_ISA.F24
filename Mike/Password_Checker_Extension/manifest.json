{
  "manifest_version": 3,
  "name": "Password Checker",
  "version": "1.0",
  "description": "Checks the strength of entered passwords",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "ExtensionIcon.png"
  }
}
