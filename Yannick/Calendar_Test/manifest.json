{
    "manifest_version": 3,
    "name": "Course Timetable",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "https://mycampus.hslu.ch/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  