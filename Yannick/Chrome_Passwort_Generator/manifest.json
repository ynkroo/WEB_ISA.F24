{
    "name": "Random password generator",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A basic extension for password generation",
    "permissions": ["scripting", "activeTab", "storage", "https://api.api-ninjas.com/*"
],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"

    }    
  }