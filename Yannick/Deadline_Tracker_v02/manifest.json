{
  "manifest_version": 3,
  "name": "Assignment Tracker",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
],
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
},
"host_permissions": [
    "<all_urls>"
]
}
